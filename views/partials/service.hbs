  
  
  <section id="services_section" class="py-4">
    <div class="container-fluid">
        
      <h1 class="text-center text-secondary">Servics We Provided</h1>
      <div class="row my-4">
        {{#each data}}
        <div class="col-md-4">
          <div class="card">
            <div class="card-body">
              <div class="text-center">
               
              <i class="{{this.icon}}  fa-3x mb-3"></i>
              </div>
              <h5 class="card-title">{{this.title}}</h5>
              <p>{{this.description}} </p>
              <a href="{{this.link}}">{{this.linkText}}</a>
              <a href="#!" class="ml-5">Support</a>
              
            </div>
          </div>
        </div>
        {{/each}}

      </div>
    </div>


  </section>
